<table name="Towary">
		<description>Tabela z danymi o towarach</description>
		<column name="Twr_TwrId" type="INTEGER" definition="IDENTITY(1,1)" filterVisible="false">
			<description>Identyfikator towaru (numer rekordu)</description>
			<details>
				Unikalny identyfikator rekordu.
				Poprzez to pole realizowane są wszystkie relacje typu 1:MANY do innych tabel.
			</details>
		</column>
		<column name="Twr_Typ" type="TINYINT" definition="NOT NULL">
			<description>Typ karty: 0 - usługa, 1 - towar</description>
		</column>
		<column name="Twr_Produkt" type="SMALLINT" definition="NOT NULL">
			<description>Czy kartoteka jest produktem</description>
			<details>
				0 - prosty &lt;br&gt;
				1 - złożony
			</details>
		</column>
		<column name="Twr_Kod" type="VARCHAR(50)" definition="NOT NULL" hash="1">
			<description>Unikalny kod towaru</description>
		</column>
		<column name="Twr_AutoKodGrupaTowarowa" type="TINYINT" definition="NOT NULL CONSTRAINT DF_Twr_AutoKodGrupaTowarowa DEFAULT(0)">
			<description>Autonumeracja kodu towaru zawiera nazwę grupy 0 - nie, 1 - tak</description>
		</column>
		<column name="Twr_AutoKodSeria" type="VARCHAR(40)" definition="NOT NULL CONSTRAINT DF_Twr_AutoKodSeria DEFAULT('')">
			<description>Autonumeracja kodu towaru seria</description>
		</column>
		<column name="Twr_AutoKodWartosc" type="VARCHAR(50)" definition="NOT NULL CONSTRAINT DF_Twr_AutoKodWartosc DEFAULT('')">
			<description>Autonumeracja kodu towaru wartosc</description>
		</column>
		<column name="Twr_AutoKodNumer" type="INTEGER" definition="NOT NULL CONSTRAINT DF_Twr_AutoKodNumer DEFAULT(0)">
			<description>Autonumeracja numer serii</description>
		</column>
		<column name="Twr_PLU" type="NVARCHAR(18)" definition="NOT NULL CONSTRAINT DF_Twr_PLU DEFAULT('')" hash="1">
			<description>Unikalny kod PLU</description>
		</column>
		<column name="Twr_NumerKat" type="NVARCHAR(40)" definition="NOT NULL" hash="1">
			<description>Numer katalogowy</description>
			<details>Pole indeksowane, pozwalające na prowadzenie równoległego, drugiego kodowania</details>
		</column>
		<column name="Twr_SWW" type="VARCHAR(20)" definition="NOT NULL">
			<description>Symbol PKWiU</description>
		</column>
		<column name="Twr_EAN" type="VARCHAR(40)" definition="NOT NULL">
			<description>Kod EAN</description>
		</column>
		<column name="Twr_Nazwa" type="NVARCHAR(255)" definition="NOT NULL" hash="1">
			<description>Pełna nazwa towaru</description>
		</column>
		<column name="Twr_NazwaFiskalna" type="NVARCHAR(40)" definition="NOT NULL CONSTRAINT DF_Twr_NazwaFiskalna DEFAULT('')">
			<description>Nazwa towaru dla drukarek/kas fiskalnych</description>
		</column>
		<column name="Twr_TwGGIDNumer" type="INTEGER" definition="NOT NULL">
			<description>Id grupy domyślnej</description>
			<details>pole jest not null - wartość 0 oznacza przynależność do roota (grupy głównej)</details>
		</column>
		<column name="Twr_URL" type="VARCHAR(254)" definition="NOT NULL" hash="1">
			<description>Adres strony w Internecie</description>
		</column>
		<column name="Twr_KatId" type="INTEGER" filterVisible="false">
			<description>Identyfikator kategorii</description>
			<details>
				Relacja do tabeli Kategorie.
				Opis (kategoria) towaru może zostać wpisany "z ręki" (Twr:KatId = 0) lub zostac pobrany z
				listy kategorii (Twr:KatId = Kat:KatId)
			</details>
		</column>
		<column name="Twr_Kategoria" type="VARCHAR(50)" definition="NOT NULL" hash="1">
			<description>Opis kategorii sprzedaży</description>
		</column>
		<column name="Twr_KatZakId" type="INTEGER">
			<description>Identyfikator dla kategorii przy zakupie</description>
		</column>
		<column name="Twr_KategoriaZak" type="VARCHAR(50)" definition="NOT NULL" hash="1">
			<description>Opis kategorii zakupu</description>
		</column>
		<column name="Twr_Opis" type="NVARCHAR(MAX)" definition="NOT NULL" hash="1">
			<description>Opis towaru</description>
		</column>
		<column name="Twr_EdycjaNazwy" type="TINYINT" definition="NOT NULL">
			<description>Zezwolenie na edycję nazwy po skopiowaniu do transakcji</description>
			<details>
				0 - nie &lt;br&gt;
				1 - tak
			</details>
		</column>
		<column name="Twr_KopiujOpis" type="TINYINT" definition="NOT NULL">
			<description>Zezwolenie na kopiowanie opisu do transakcji</description>
			<details>
				0 - nie &lt;br&gt;
				1 - tak
			</details>
		</column>
		<column name="Twr_EdycjaOpisu" type="TINYINT" definition="NOT NULL">
			<description>Zezwolenie na edycję opisu po skopiowaniu do transakcji</description>
			<details>
				0 - nie &lt;br&gt;
				1 - tak
			</details>
		</column>
		<column name="Twr_JM" type="NVARCHAR(20)" definition="NOT NULL">
			<description>Nazwa podstawowej jednostki miary</description>
		</column>
		<column name="Twr_JMCalkowite" type="TINYINT" definition="NOT NULL">
			<description>Format jednostki miary</description>
			<details>Format przeliczania jednostek miar. 1 oznacza, że przelicznik będzie całkowity</details>
		</column>
		<column name="Twr_JMZ" type="NVARCHAR(20)" definition="NOT NULL CONSTRAINT DF_Twr_JMZ DEFAULT('')">
			<description>Nazwa zbiorczej jednostki miary</description>
		</column>
		<column name="Twr_JMPrzelicznikL" type="DECIMAL(15,2)" definition="NOT NULL CONSTRAINT DF_Twr_JMPrzelicznikL DEFAULT(1)">
			<description>Licznik przelicznika JM</description>
			<details>Licznik przelicznika między podstawową a zbiorczą jednostką miary</details>
		</column>
		<column name="Twr_JMPrzelicznikM" type="DECIMAL( 7,0)" definition="NOT NULL CONSTRAINT DF_Twr_JMPrzelicznikM DEFAULT(1)">
			<description>Mianownik przelicznika JM</description>
			<details>Mianownik przelicznika między podstawową a zbiorczą jednostką miary</details>
		</column>
		<column name="Twr_Kaucja" type="TINYINT" definition="NOT NULL">
			<description>Czy towar jest opakowaniem/kaucją</description>
			<details>
				0 - towar zwykły &lt;br&gt;
				1 - opakowanie
			</details>
		</column>
		<column name="Twr_TwCNumer" type="INTEGER" definition="NOT NULL">
			<description>Domyślna cena dla towaru</description>
			<details>Numer domyślnej ceny, związany z tabelą TwrCeny</details>
		</column>
		<column name="Twr_UdostepniajWCenniku" type="TINYINT" definition="NOT NULL">
			<description>Czy towar ma być udostępniany w cenniku</description>
			<details>
				0 - nie udostępniaj
				1 - udostępniaj
			</details>
		</column>
		<column name="Twr_Waluta" type="VARCHAR(3)" definition="NOT NULL">
			<description>Waluta ostatniej ceny zakupu</description>
		</column>
		<column name="Twr_KursNumer" type="INTEGER" definition="NOT NULL">
			<description>Numer kursu, po którym nastapił ostatni zakup towaru</description>
		</column>
		<column name="Twr_KursL" type="DECIMAL(15,4)" definition="NOT NULL">
			<description>Licznik notowania ostatniej ceny zakupu</description>
		</column>
		<column name="Twr_KursM" type="DECIMAL( 5,0)" definition="NOT NULL">
			<description>Mianownik notowania ostatniej ceny zakupu</description>
		</column>
		<column name="Twr_MinCenaMarza" type="DECIMAL(15,3)" definition="NOT NULL">
			<description>Wartość minimalnej ceny sprzedaży lub minimalnej marży</description>
		</column>
		<column name="Twr_TypMinimum" type="TINYINT" definition="NOT NULL">
			<description>Typ minimum</description>
			<details>0-brak, 1-minimalna marża, 2-minimalna cena</details>
		</column>
		<column name="Twr_KosztUslugiWal" type="DECIMAL(17,4)" definition="NOT NULL">
			<description>Koszt usługi wyrażony w walucie, dla kosztów kwotowych</description>
		</column>
		<column name="Twr_KosztUslugi" type="DECIMAL(17,4)" definition="NOT NULL">
			<description>Koszt usługi</description>
		</column>
		<column name="Twr_KosztUslugiTyp" type="TINYINT" definition="NOT NULL">
			<description>Typ kosztu usługi: 1 - kwotowy, 2 - procentowy</description>
		</column>
		<column name="Twr_KosztKGO" type="DECIMAL(15,2)" definition="NOT NULL CONSTRAINT DF_Twr_KosztKGO DEFAULT(0)">
			<description>KGO</description>
		</column>
		<column name="Twr_SONId" type="INTEGER">
			<description>Wskaźnik do słownika schematów opakowań</description>
		</column>
		<column name="Twr_KntId" type="INTEGER">
			<description>Identyfikator dostawcy towaru</description>
			<details>Wskaźnik do tabeli Kontrahenci</details>
		</column>
		<column name="Twr_KodDostawcy" type="NVARCHAR(50)" definition="NOT NULL" hash="1">
			<description>Kod towaru u dostawcy</description>
		</column>
		<column name="Twr_ProducentKod" type="NVARCHAR(50)" definition="NOT NULL CONSTRAINT DF_Twr_ProducentKod DEFAULT('')" hash="1">
			<description>Kod towaru producenta</description>
		</column>
		<column name="Twr_IloscMin" type="DECIMAL(15,4)" definition="NOT NULL">
			<description>Ilość minimalna towaru na magazynie</description>
		</column>
		<column name="Twr_IloscMinJM" type="NVARCHAR(20)" definition="NOT NULL">
			<description>Jednostka miary dla ilości minimalnej</description>
		</column>
		<column name="Twr_IloscMax" type="DECIMAL(15,4)" definition="NOT NULL">
			<description>Ilość maksymalna towaru na magazynie</description>
		</column>
		<column name="Twr_IloscMaxJM" type="NVARCHAR(20)" definition="NOT NULL">
			<description>Jednostka miary dla ilości maksymalnej</description>
		</column>
		<column name="Twr_IloscZam" type="DECIMAL(15,4)" definition="NOT NULL">
			<description>Ilość zamawiana jednorazowo</description>
		</column>
		<column name="Twr_IloscZamJM" type="NVARCHAR(20)" definition="NOT NULL">
			<description>jednostka miary dla zamówień</description>
		</column>
		<column name="Twr_Stawka" type="DECIMAL( 5,2)" definition="NOT NULL">
			<description>Wartość procentowa stawki VAT</description>
		</column>
